<robot name="f550_visualization">
  <material name="GrayTransparent">
    <color rgba="1 8e-1 8e-1 8e-1"/>
  </material>
  <material name="Red">
    <color rgba="1 0 0 1"/>
  </material>
  <material name="Gray">
    <color rgba="8e-1 8e-1 8e-1 1"/>
